.home-page {
 .at-grid-item__content {
    box-sizing: border-box;
    padding: 2.4vw;
  }

  .at-grid__flex {
    .content-inner__icon {
      -webkit-flex: auto;
      -ms-flex: auto;
      flex: auto;
      width: 100%;
      border-radius: 16px;
      overflow: hidden;
      background-color: #f6f6f6;
    }

    .content-inner__img {
      width: 100%;
      height: 100%;
    }
  }
}